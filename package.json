{
  "name": "sites-main",
  "version": "0.0.1",
  "description": "个人开发网站",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"watch-ts,watch-node,Client\" -c \"cyan.bold,green.bold,yellow.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run client-dev\"",
    "watch-node": "nodemon --inspect dist/server/index.js",
    "watch-ts": "tsc -w",
    "dist-ts": "tsc",
    "client-dev": "node ./build/development.ts",
    "client-prod": "node ./build/production.ts",
    "webpack": "webpack --config ./build/webpack.config.prod.ts",
    "dist": "concurrently --kill-others-on-fail -p \"[{name}]\" -n \"dist-ts,client-prod,webpack\" -c \"cyan.bold,green.bold,yellow.bold\" \"npm run dist-ts\" \"npm run client-prod\" \"npm run webpack\""
  },
  "keywords": [
    "koa",
    "egg",
    "react",
    "graphQL",
    "typescript"
  ],
  "author": "hzsuqin@corp.netease.com",
  "dependencies": {
    "@types/http-errors": "^1.6.1",
    "@types/koa": "^2.0.45",
    "@types/koa-router": "^7.0.28",
    "@types/lodash": "^4.14.109",
    "@types/mongoose": "^5.0.14",
    "@types/node": "^10.1.2",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-router-dom": "^4.2.6",
    "axios": "^0.18.0",
    "koa": "^2.3.0",
    "koa-better-body": "^3.0.4",
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.2",
    "less": "^2.7.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.11.6",
    "qs": "^6.5.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "browser-sync": "^2.24.4",
    "bs-fullscreen-message": "^1.1.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "nodemon": "^1.17.4",
    "shelljs": "^0.8.2",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3"
  }
}
