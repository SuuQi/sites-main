{
  "name": "sites-main",
  "version": "1.0.0",
  "description": "sites-main",
  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"client-dev,server-dev\" -c \"cyan.bold,green.bold\" \"npm run client-dev\" \"npm run server-dev\"",
    "dist": "npm run client-dist-assets && npm run client-dist-webpack && npm run server-dist",
    "client-dev": "node ./build/development.js",
    "client-dist-assets": "node ./build/production.js",
    "client-dist-webpack": "node --max_old_space_size=512 node_modules/webpack/bin/webpack.js --config ./build/webpack.config.prod.js",
    "server-dev": "egg-bin dev -r egg-ts-helper/register --port=9000 --ts",
    "server-dist": "ets && tsc -p tsconfig.json",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint --fix && npm run test-local",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "start": "egg-scripts start --title=pangu-sites-main --port=9000"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:SuuQi/sites-main.git"
  },
  "author": "15957136119@163.COM",
  "dependencies": {
    "egg": "^2.6.1",
    "egg-mongoose": "^3.1.0",
    "egg-router-decorator": "^1.0.0",
    "egg-scripts": "^2.6.0",
    "egg-view-ejs": "^2.0.0",
    "koa-compose": "^4.1.0",
    "koa-logger": "^3.2.0",
    "moment": "^2.18.1",
    "request": "^2.88.0",
    "xlsx": "^0.14.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/http-errors": "^1.6.1",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/prismjs": "^1.9.0",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-router-dom": "^4.2.6",
    "@types/supertest": "^2.0.0",
    "autoprefixer-loader": "^3.2.0",
    "axios": "^0.18.0",
    "browser-sync": "^2.24.4",
    "bs-fullscreen-message": "^1.1.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.0.1",
    "css-loader": "^0.28.11",
    "egg-bin": "^4.6.2",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.11.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.10",
    "prismjs": "^1.15.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.3.0",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^3.0.0",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.1.3"
  }
}
